

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Dr. Rishi Dwivedi Baby Care Chhatarpur M.P. India</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="baby care" name="keywords">
    <meta content="best baby care" name="description">

    <link href="~/Images/MainImage/favicon.png" rel="icon">
    <link href="~/Images/MainImage/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Fonts -->

    <link href="~/Content/maincss/font-awesomemain.css" rel="stylesheet" />
    <!-- Vendor CSS Files -->
    <link href="~/Content/maincss/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/maincss/bootstrap-icons.css" rel="stylesheet" />
    <link href="~/Content/maincss/aos.css" rel="stylesheet" />
    <link href="~/Content/maincss/all.css" rel="stylesheet" />
    <link href="~/Content/maincss/glightbox.css" rel="stylesheet" />
