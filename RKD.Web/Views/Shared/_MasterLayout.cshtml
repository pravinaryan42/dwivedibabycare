    <link href="~/Content/maincss/main.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>

<body>
    @Html.Partial("_MasterHeader")
    @RenderBody()
    @{ Html.RenderPartial("_MasterFooter");}
    <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Preloader -->
    <div id="preloader"></div>

    <!-- JavaScript Libraries -->
    <script src="~/js/MainJs/bootstrap.bundle.min.js"></script>
    <script src="~/js/MainJs/validate.js"></script>
    <script src="~/js/MainJs/aos.js"></script>
    <script src="~/js/MainJs/glightbox.js"></script>
    <script src="~/js/MainJs/purecounter_vanilla.js"></script>
    <script src="~/js/MainJs/swiper-bundle.min.js"></script>

